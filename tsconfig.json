{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/client/*": ["./client/*"],
      "@/modules/*": ["./client/modules/*"],
  "@/modules": ["./client/modules"],
      "@/ui-kit/*": ["./client/ui-kit/*"],
  "@/layout/*": ["./client/layout/*"],
      "@/integrations/*": ["./server/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
